ifs_data <- function(indicator,
country,
start,
end,
freq){
imfr::imf_data(database_id = "IFS",
indicator = indicator,
country = country,
start = start,
end = end,
freq = freq,
return_raw = F,
print_url = F,
times = 3)
}
